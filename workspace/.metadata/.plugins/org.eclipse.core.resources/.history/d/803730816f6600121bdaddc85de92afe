fmod BASE is

	pr NAT .
	pr QID .
	pr STRING .
	
	sort Class .
	sort Object .
	
	op null : -> Object [ctor] .
	
	op [_{_}] : Class Object -> Object [ctor] . 
	
	sorts Car Porsche .
	subsort Porsche < Car .
	
	subsort Car < Class .
	subsort Porsche < Class .
	
	
	var S : String .
	var N : Nat .
	
	op car : String -> Object [ctor] .
	op newCar : String -> Car .
	eq newCar(S) = [car(S){null}] .
	
endfm

rew newCar("car")  .