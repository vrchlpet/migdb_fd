(rew < 'multProp : MultipliableProperty | name : "multProp", isOrdered : false, isUnique : false, lowerBound : 1, upperBound : 5, owningClass : 'ownCls > setOwningClass('multProp, 'newOwnCls) .)

load app_EmbaddedClass.maude .
