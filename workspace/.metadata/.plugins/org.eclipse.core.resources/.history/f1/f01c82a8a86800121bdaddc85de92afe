view PrimTypes from TRIV to BASE is
	sort Elt to PrimitiveType .
endv

fmod BASE is

	sort PrimitiveType .
	op INT BOOL NAT CHAR : -> PrimitiveType .
	
	var PT : PrimitiveType .
	var PTL : List{PrimTypes} .
	
	op add : PrimitiveType List{PrimTypes} -> List{PrimTypes} .
	eq add(PT, PTL) = PT PTL .


endfm




