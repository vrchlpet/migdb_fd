(omod INDEX is

	pr BASE .
	
	msg setName : Oid String -> Msg .
	msg addColumn : Oid Oid -> Msg .
	msg setOwningSchema : Oid Oid -> Msg .

	vars M N : Oid .
	var S : String .
	var OS : OidSeq .

	rl [setName] : setName(M,N) < M : Index | > => < M : Index | name : S > .
	rl [addColumn] : addColumn(M,N)
endom)